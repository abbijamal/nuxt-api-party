{
  "name": "nuxt-api-party",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.13.2",
  "description": "Nuxt 3 module to connect with any API securely",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-api-party#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-api-party.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-api-party/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "api",
    "composable",
    "query",
    "fetch"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.11",
    "defu": "^6.1.0",
    "ohash": "^0.1.5",
    "ohmyfetch": "^0.4.19",
    "scule": "^0.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@nuxt/module-builder": "^0.1.7",
    "bumpp": "^8.2.1",
    "eslint": "^8.24.0",
    "nuxt": "^3.0.0-rc.11",
    "nuxt-api-party": "workspace:*",
    "typescript": "^4.8.4",
    "vue-tsc": "^1.0.0"
  }
}
